--Additional Codes--

--[[Remove kill function from laser doors + Collect-Cash
Collects all tools + Remove-WALLS
Remove Ching sound (ToggleKey = R)]] 
local function tacking(actionName,inputState)
    if actionName == "Tack1" then
        if inputState == Enum.UserInputState.Begin then


            --CollectsAllTools--
            for i, v in ipairs(game.Workspace:GetDescendants("Tycoons")) do
                if v:IsA("Part") and v.Name == "Neon" then
                    firetouchinterest(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart, v, 0)
                end
            end
	--Remove-Walls--
	for i,w in pairs(game.Workspace.Tycoons:GetDescendants()) do
		if w.Name:match("Wall1") or  w.Name:match("Wall2") or  w.Name:match("Wall3") or  w.Name:match("Wall4") or  w.Name:match("Wall5") or  w.Name:match("Wall6") or  w.Name:match("Wall7") then
			w:Remove()
		end
	end
            --RemovesKillingFunctionFromLaserDoors--
            for i, y in ipairs(game.Workspace:GetDescendants("Tycoons")) do
                if y:IsA("Part") and y.Name == "ActDoor" then
					if game.Players.LocalPlayer.leaderstats.Cash.Value ~= 0 then
                    y:Destroy()
					end
                end
            end


            --RemovesDingSoundWhenCollectingCash--
            for i, v in ipairs(game.Workspace:GetDescendants("Tycoons")) do
                if v:IsA("Sound") then
                    v:Destroy()
                end
            end


--Auto Collect Cash--
function get_user_tycoon()
    for i, v in pairs(workspace.Tycoons:GetChildren()) do
        if v:FindFirstChild("isim") then
            if v.isim.Value == game.Players.LocalPlayer.Name then
                return v
            end
        end
    end
end

function run()
    local user_tycoon = get_user_tycoon()
    local cash_register = user_tycoon.CashRegister.Ching
    local ultima_cash_register = user_tycoon.UltimaPad.CashRegister2.Ching
    
    cash_register.Transparency = 1
    cash_register.CanCollide = false

    ultima_cash_register.Transparency = 1
    ultima_cash_register.CanCollide = false
            spawn(function()
                while true do
                    cash_register.Position = game.Players.LocalPlayer.Character.Head.Position 
                    ultima_cash_register.Position = game.Players.LocalPlayer.Character.Head.Position
                    wait(0.5)
                end
            end)


end
run()
			

		end
	end
end
game:GetService("ContextActionService"):BindAction("Tack1", tacking, false, Enum.KeyCode.R)


--[[SPAM-AIM-KILL-(ToggleKey = Z)]] 

local function tacking(actionName,inputState)
    if actionName == "Tack2" then
        if inputState == Enum.UserInputState.Begin then
			stop = 1
			local User = game.Players.LocalPlayer
				for i, v in next, User.Backpack:GetChildren() do
					local Remote = v:FindFirstChild"RemoteEvent"
					if v.Name ~= "Eye Teleport" and Remote and User.Character and not tonumber(v) and stop ~= 0 then
						User.Character.Humanoid:EquipTool(v)
						v:Activate()
						wait(0.3)
						User.Character.Humanoid:UnequipTools()
					end
				end
			end
		end
	end
game:GetService("ContextActionService"):BindAction("Tack2", tacking, false, Enum.KeyCode.Z)

--[[Stop-SPAM-AIM-KILL-(ToggleKey = X)]] 
local function tacking(actionName,inputState)
    if actionName == "Tack3" then
        if inputState == Enum.UserInputState.Begin then
				stop = 0
			end
		end
	end
game:GetService("ContextActionService"):BindAction("Tack3", tacking, false, Enum.KeyCode.X)

--Auto-upgrade--
local function tacking(actionName,inputState)
    if actionName == "Tack4" then
        if inputState == Enum.UserInputState.Begin then
			spawn(function ()
				
			function get_user_tycoon() --Detect-Tycoon-Name
				for i, v in pairs(workspace.Tycoons:GetChildren()) do
					if v:FindFirstChild("isim") then
						if v.isim.Value == game.Players.LocalPlayer.Name then
							return v
						end
					end
				end
			end
	
	function run() --Runner
		local function iterate_in(object)
			for i, v in pairs(object:GetChildren()) do
				if v.Name == "Touch" then
					if v.Parent.Name:match("Robux") or v.Parent.Name:match("Shop") then
					else
						v.CanCollide = false
						v.Transparency = 1
						v.Position = game.Players.LocalPlayer.Character.Head.Position
					end
				end
	
				if v:IsA("BillboardGui") then
					v:Remove()
				end

				for i, s in pairs(game.Workspace.Tycoons:GetDescendants()) do
					if s:IsA("Sound") and s.Name == "Error" then
						s:Destroy()
					end
				end

				wait(0.1)
				iterate_in(v)
			end
		end
		local user_tycoon = get_user_tycoon()
		while true do
			iterate_in(user_tycoon)
			wait(0.5)
		end
	end
run()
end)
end
end
end
game:GetService("ContextActionService"):BindAction("Tack4", tacking, false, Enum.KeyCode.U)
