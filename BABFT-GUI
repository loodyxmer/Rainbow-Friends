farm.Name = "farm"
farm.Parent = main
farm.BackgroundColor3 = Color3.fromRGB(0, 255, 238)
farm.Position = UDim2.new(0.140449435, 0, 0.421768725, 0)
farm.Size = UDim2.new(0, 64, 0, 22)
farm.Font = Enum.Font.Fantasy
farm.Text = "Auto-Farm"
farm.TextColor3 = Color3.fromRGB(0, 0, 0)
farm.TextSize = 14.000
farm.Visible = true
farm.MouseButton1Down:connect(
    function()
        farm.Visible = false
        nof.Visible = true
        for i, v in pairs(game:GetService("Workspace").BoatStages.NormalStages:GetChildren()) do
            wait(0)
            if v.Name:match("CaveStage") then
                v.DarknessPart.Transparency = 0.8
                v.DarknessPart.Color = Color3.fromRGB(255, 100, 100)
                v.DarknessPart.Material = "Neon"
            end
        end
        getgenv().farm = true
        loadstring(game:HttpGet("https://raw.githubusercontent.com/loodyxmer/Scripts/main/auto-farm"))()
    end
)

farm.MouseEnter:Connect(
    function()
        title.Text = "Auto-Farm!"
        title.TextColor3 = Color3.fromRGB(40, 89, 40)
    end
)

farm.MouseLeave:Connect(
    function()
        title.Text = "Welcome To Loody's GUI"
        title.TextColor3 = Color3.fromRGB(89, 34, 89)
    end
)

nof.Name = "nof"
nof.Parent = main
nof.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
