nof.Text = "STOP-F"
nof.TextColor3 = Color3.fromRGB(0, 0, 0)
nof.TextSize = 14.000
nof.Visible = false
nof.MouseButton1Down:connect(
    function()
        nof.Visible = false
        getgenv().farm = false
        game.Players.LocalPlayer.Character.Head:Destroy()
        farm.Visible = true
    end
)

